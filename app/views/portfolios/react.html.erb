<h2>React </h2>
<% @react_portfolio_items.each do |item| %>
<h2><%=link_to item.title, portfolio_show_path(item) %></h2>
<h4><%= item.subtitle %></h4>
<p><%= item.body %></p>
<%= image_tag item.thumb_image unless item.thumb_image.nil?%>
<%= link_to 'edit',  edit_portfolio_path(item) %>
<%= link_to 'remove',  portfolio_path(item), method: :delete , data: { confirm: 'Are you sure?' } if logged_in?(:site_admin) if logged_in?(:site_admin)%>
<% end %>